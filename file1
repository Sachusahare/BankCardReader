scanf("%d",&a);
printf("Enter the value of 'B' : ");
scanf("%d",&b);
if(a > b)
printf(" The bigger value = %d and the smaller value = %d",a,b);
else
printf(" The bigger value = %d and the smaller value = %d",b,a);
getch();
}

